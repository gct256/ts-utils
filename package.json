{
  "name": "@gct256/ts-utils",
  "version": "2.1.1",
  "description": "my TypeScript utilities.",
  "main": "index",
  "typings": "types/index.d.ts",
  "scripts": {
    "test": "jest",
    "prebuild": "rimraf types index.js index.mjs",
    "build": "rollup -c",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gct256/ts-utils.git"
  },
  "author": "gct256+github@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gct256/ts-utils/issues"
  },
  "homepage": "https://github.com/gct256/ts-utils#readme",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "eslint": "^7.28.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "jest": "^27.0.4",
    "jest-canvas-mock": "^2.3.1",
    "prettier": "^2.3.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.51.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^27.0.3",
    "tslib": "^2.2.0",
    "typescript": "^4.3.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "\\.rpt2_cache"
    ],
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testURL": "http://localhost/"
  }
}
