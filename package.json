{
  "name": "@gct256/ts-utils",
  "version": "1.2.0",
  "description": "my utils.",
  "main": "index",
  "typings": "types/index.d.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "build": "rollup -c",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gct256/ts-utils.git"
  },
  "author": "gct256+github@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gct256/ts-utils/issues"
  },
  "homepage": "https://github.com/gct256/ts-utils#readme",
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "jest": "^24.8.0",
    "jest-canvas-mock": "^2.1.0",
    "rollup": "^1.16.6",
    "rollup-plugin-typescript2": "^0.21.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^3.5.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "\\.rpt2_cache"
    ],
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testURL": "http://localhost/"
  }
}
